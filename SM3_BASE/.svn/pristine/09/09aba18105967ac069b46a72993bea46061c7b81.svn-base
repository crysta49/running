package com.srpost.cm.bo.base.faq.ctg;

import org.apache.ibatis.type.Alias;

import com.srpost.salmon.bean.BaseTreeBean;
import com.srpost.salmon.constant.Constant;

/**
 * 내부단 FAQ분류 정보 Bean
 *
 * @author  finkle
 * @date    2014-11-20
 * @since   2.0
 */
@Alias("faqCtgBean")
@SuppressWarnings("serial")
public class FaqCtgBean extends BaseTreeBean {
    
    /** 디폴트 최상위 분류코드 */
    private String topCtgCd = Constant.TOP_CTG_CD;
    
    /** 카테고리_코드 */
    private Integer ctgCd;
    /** 카테고리_명 */
    private String ctgNm;
    /** 상위_카테고리_코드 */
    private Integer highCtgCd;
    /** 상위_카테고리_명 */
    private String highCtgNm;
    /** 정렬_순서 */
    private Integer orderNo;
    /** 사용_여부 */
    private String useYn;
    /** 등록_일시 */
    private String regDt;
    /** 수정_일시 */
    private String modiDt;
    
    
    public String getTopCtgCd() {
        return topCtgCd;
    }
    public void setTopCtgCd(String topCtgCd) {
        this.topCtgCd = topCtgCd;
    }
    public Integer getCtgCd() {
        return ctgCd;
    }
    public void setCtgCd(Integer ctgCd) {
        this.ctgCd = ctgCd;
    }
    public String getCtgNm() {
        return ctgNm;
    }
    public void setCtgNm(String ctgNm) {
        this.ctgNm = ctgNm;
    }
    public Integer getHighCtgCd() {
        return highCtgCd;
    }
    public void setHighCtgCd(Integer highCtgCd) {
        this.highCtgCd = highCtgCd;
    }
    public String getHighCtgNm() {
        return highCtgNm;
    }
    public void setHighCtgNm(String highCtgNm) {
        this.highCtgNm = highCtgNm;
    }
    public Integer getOrderNo() {
        return orderNo;
    }
    public void setOrderNo(Integer orderNo) {
        this.orderNo = orderNo;
    }
    public String getUseYn() {
        return useYn;
    }
    public void setUseYn(String useYn) {
        this.useYn = useYn;
    }
    public String getRegDt() {
        return regDt;
    }
    public void setRegDt(String regDt) {
        this.regDt = regDt;
    }
    public String getModiDt() {
        return modiDt;
    }
    public void setModiDt(String modiDt) {
        this.modiDt = modiDt;
    }
}
