package com.srpost.cm.bo.base.mgr.absence;

import org.apache.ibatis.type.Alias;

import com.srpost.salmon.bean.BaseBean;

/**
 * 내부단 직원 부재정보 Bean
 *
 * @author  finkle
 * @date    2014-12-17
 * @since   3.0
 */
@Alias("mgrAbsenceBean")
@SuppressWarnings("serial")
public class MgrAbsenceBean extends BaseBean {
    
    /** 직원_아이디 */
    private String mgrId;
    /** 직원_명 */
    private String mgrNm;
    /** 대리인_아이디 */
    private String agencyId;
    /** 대리인_명 */
    private String agencyNm;
    /** 부재_코드 */
    private String absenceCd;
    /** 부재_명 */
    private String absenceNm;
    /** 부재_사유 */
    private String absenceReason;
    /** 시작_일시 */
    private String startDt;
    /** 종료_일시 */
    private String endDt;
    /** 시작_일시 Plain */
    private String startDtPlain;
    /** 종료_일시 Plain */
    private String endDtPlain;
    /** 등록_일시 */
    private String regDt;
    /** 수정_일시 */
    private String modiDt;
    
    
    public String getMgrId() {
        return mgrId;
    }
    public void setMgrId(String mgrId) {
        this.mgrId = mgrId;
    }
    public String getMgrNm() {
        return mgrNm;
    }
    public void setMgrNm(String mgrNm) {
        this.mgrNm = mgrNm;
    }
    public String getAgencyId() {
        return agencyId;
    }
    public void setAgencyId(String agencyId) {
        this.agencyId = agencyId;
    }
    public String getAgencyNm() {
        return agencyNm;
    }
    public void setAgencyNm(String agencyNm) {
        this.agencyNm = agencyNm;
    }
    public String getAbsenceCd() {
        return absenceCd;
    }
    public void setAbsenceCd(String absenceCd) {
        this.absenceCd = absenceCd;
    }
    public String getAbsenceNm() {
        return absenceNm;
    }
    public void setAbsenceNm(String absenceNm) {
        this.absenceNm = absenceNm;
    }
    public String getAbsenceReason() {
        return absenceReason;
    }
    public void setAbsenceReason(String absenceReason) {
        this.absenceReason = absenceReason;
    }
    public String getStartDt() {
        return startDt;
    }
    public void setStartDt(String startDt) {
        this.startDt = startDt;
    }
    public String getEndDt() {
        return endDt;
    }
    public void setEndDt(String endDt) {
        this.endDt = endDt;
    }
    public String getRegDt() {
        return regDt;
    }
    public void setRegDt(String regDt) {
        this.regDt = regDt;
    }
    public String getStartDtPlain() {
        return startDtPlain;
    }
    public void setStartDtPlain(String startDtPlain) {
        this.startDtPlain = startDtPlain;
    }
    public String getEndDtPlain() {
        return endDtPlain;
    }
    public void setEndDtPlain(String endDtPlain) {
        this.endDtPlain = endDtPlain;
    }
    public String getModiDt() {
        return modiDt;
    }
    public void setModiDt(String modiDt) {
        this.modiDt = modiDt;
    }
}
