.smgrid .x-grid-header-ct, 
.smgrid .x-toolbar-default { background: none; background-color: #F8F8F8; }
.smgrid .no-use .x-grid-cell { text-decoration: line-through; }
.smgrid .x-action-col-icon { margin: 0 2px; }
.smgrid .x-grid-cell-special { /* background-color: transparent; */ background-image: none; }
.smgrid .exceed .x-grid-cell { background-color: #FFB0C4; }
.smgrid .total .x-grid-cell {background-color: #FFF118;}

.smtree { }
.smtree .x-tree-disabled, .x-tree-disabled { color: #B2B2B2; }
.smtree .x-tree-disabled .x-tree-checkbox { display: none; }
.smtree .x-grid-header-ct { border: none; }
.smtree .x-panel-header-default, .smgrid .x-panel-header-default { height: 30px; margin-bottom: 6px; border: none; background: none; padding: 4px 0 3px 0; }
.smtree .x-panel-body-default { border: 0; }
.smtree .x-tree-view { border: 1px solid #E3E3E3; border-top: 2px solid #3B92DB; }

.smtreegrid .x-grid-tree-node-expanded .x-grid-cell { background-color: #F4F4F4; border-top-color: #EDEDED; border-bottom-color: #EDEDED; }
.smtreegrid .x-grid-row .x-grid-cell { border-right: 1px solid #D0D0D0; }

.smtreecombo [class^=x-], .smdatepicker [class^=x-] { padding: 0; height: 30px; }
.smdatepicker .x-form-field,
.smtreecombo .x-form-field { padding: 2px 7px; border-color: #ccc; background: none; }
.smtreecombo .x-form-trigger { background-image: url("../images/shared/trigger.gif"); }
.smdatepicker .x-form-date-trigger { background-image: url("../images/shared/date-trigger.gif"); }
.smdatepicker td, .smtreecombo td { border: none; }

.x-boundlist-item { white-space: nowrap; }  /* Combobox */

/* ExtJS : Modal window */
.x-window-header-default, .x-window-default { box-shadow: none; border-radius: 0; border: 1px solid #c7c7c7; }
.x-window-header-default .x-tool-img { background-color: transparent; }
.x-window-header-default .x-header-text {  }
.x-window-body-default { /* border-top: 3px solid #4191D5; */ }
.x-css-shadow { display: none !important; }

/*----------------------------------------
    SyntaxHighlighter
----------------------------------------*/
.dp-highlighter { word-break: break-all; word-wrap: break-word; }

/*----------------------------------------
    IMPRV STAT
----------------------------------------*/
.grouped-list .x-boundlist-item {	padding: 1px 3px 0 10px;}
.grouped-list .group-header {  padding: 4px;  font-weight: bold;  font-size:14px;  border-bottom: 1px solid #ddd;}
.multiSelect .x-form-trigger-wrap .x-trigger-cell .x-form-trigger { height: 30px;}

