body.srm div.sr-top #mypage-pop, body.srm div.sr-top #tot-menu-pop { display: none; z-index: 100; position: absolute; top: 25px; right: 0; text-align: left; white-space: nowrap; border: 2px solid #337ab7; background-color: #fff; }
