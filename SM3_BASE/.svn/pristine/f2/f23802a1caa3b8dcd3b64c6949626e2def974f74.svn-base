package com.srpost.cm.bo.base.auth;

import org.apache.ibatis.type.Alias;

import com.srpost.salmon.bean.BaseBean;

/**
 * 내부단 권한그룹_메뉴_맵핑 정보 Bean
 *
 * @author  finkle
 * @date    2014-11-06
 * @since   3.0
 */
@Alias("authItemBean")
@SuppressWarnings("serial")
public class AuthItemBean extends BaseBean {
    
    /** 권한_코드 */
    private Integer authCd;
    /** 권한_명 */
    private String authNm;
    /** 메뉴_코드 */
    private Integer menuCd;
    /** 메뉴_명 */
    private String menuNm;
    /** 조회_여부 */
    private String readYn;
    /** CREATE_여부 */
    private String createYn;
    /** UPDATE_여부 */
    private String updateYn;
    /** DELETE_여부 */
    private String deleteYn;
    
    /** 메뉴_패터 */
    private String menuPattern;
    
    
    public Integer getAuthCd() {
        return authCd;
    }
    public void setAuthCd(Integer authCd) {
        this.authCd = authCd;
    }
    public String getAuthNm() {
        return authNm;
    }
    public void setAuthNm(String authNm) {
        this.authNm = authNm;
    }
    public Integer getMenuCd() {
        return menuCd;
    }
    public void setMenuCd(Integer menuCd) {
        this.menuCd = menuCd;
    }
    public String getMenuNm() {
        return menuNm;
    }
    public void setMenuNm(String menuNm) {
        this.menuNm = menuNm;
    }
    public String getReadYn() {
        return readYn;
    }
    public void setReadYn(String readYn) {
        this.readYn = readYn;
    }
    public String getCreateYn() {
        return createYn;
    }
    public void setCreateYn(String createYn) {
        this.createYn = createYn;
    }
    public String getUpdateYn() {
        return updateYn;
    }
    public void setUpdateYn(String updateYn) {
        this.updateYn = updateYn;
    }
    public String getDeleteYn() {
        return deleteYn;
    }
    public void setDeleteYn(String deleteYn) {
        this.deleteYn = deleteYn;
    }
    public String getMenuPattern() {
        return menuPattern;
    }
    public void setMenuPattern(String menuPattern) {
        this.menuPattern = menuPattern;
    }
}
