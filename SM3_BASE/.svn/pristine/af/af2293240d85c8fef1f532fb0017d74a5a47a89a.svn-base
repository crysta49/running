<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
    version="2.0">

    <description>Salmon JSP Tag Library</description>
    <tlib-version>3.0</tlib-version>
    <short-name>sm</short-name>
    <uri>http://www.srpost.co.kr/jsp/taglib</uri>
    
    <!-- UTIL -->
    
    <tag>
        <name>static</name>
        <tag-class>com.srpost.salmon.web.tag.util.StaticClassTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>clazz</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>var</name>
            <required>true</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <name>js</name>
        <tag-class>com.srpost.salmon.web.tag.util.JSIncludeTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>delimeter ','</description>
            <name>names</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <name>css</name>
        <tag-class>com.srpost.salmon.web.tag.util.CSSIncludeTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>delimeter ','</description>
            <name>names</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    
    <!-- UI -->
    
    <tag>
        <name>button-layout</name>
        <tag-class>com.srpost.salmon.web.tag.ui.ButtonLayoutTag</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
            <description>left | right(default) | center | both</description>
            <name>align</name>
            <required>false</required>
        </attribute>
        <attribute>
            <description>true | false(default)</description>
            <name>group</name>
            <required>false</required>
        </attribute>
    </tag>
    
    <tag>
        <name>button</name>
        <tag-class>com.srpost.salmon.web.tag.ui.ButtonTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>button(default) | submit</description>
            <name>type</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>id</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>msgKey</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>icon</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>glyph</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>tip</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>onclick</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>cssClass</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>badge</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>btn-lg | btn-md | btn-sm(default) | btn-xs</description>
            <name>btnSize</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <name>code</name>
        <tag-class>com.srpost.salmon.web.tag.ui.CodeTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>select(default) | checkbox | radio</description>
            <name>type</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>name</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>id</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>grpCd</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>defaultLabel</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>defaultValues</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>excludeValues</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>size</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>cssClass</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>option</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>onclick</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>onchange</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <name>file-list</name>
        <tag-class>com.srpost.salmon.web.tag.ui.FileListTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>list</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>true | false(default)</description>
            <name>checkbox</name>
            <required>false</required>
        </attribute>
        <attribute>
            <description>true(default) | false</description>
            <name>downCnt</name>
            <required>false</required>
        </attribute>
        <attribute>
            <description>true(default) | false</description>
            <name>fileSize</name>
            <required>false</required>
        </attribute>
        <attribute>
            <description>true(default) | false</description>
            <name>fileType</name>
            <required>false</required>
        </attribute>
        <attribute>
            <description>true(default) | false</description>
            <name>fileLog</name>
            <required>false</required>
        </attribute>
        <attribute>
            <description>true | false(default)</description>
            <name>noData</name>
            <required>false</required>
        </attribute>
        <attribute>
            <description>true(default) | false</description>
            <name>useZip</name>
            <required>false</required>
        </attribute>
        <attribute>
            <description>zip download text</description>
            <name>zipText</name>
            <required>false</required>
        </attribute>
    </tag>
    
    <tag>
        <name>br</name>
        <tag-class>com.srpost.salmon.web.tag.ui.BrTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>data</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>        
    
    <tag>
        <name>no-data</name>
        <tag-class>com.srpost.salmon.web.tag.ui.NoDataTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>dataBean</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>colspan</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>msg</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <name>fix-length</name>
        <tag-class>com.srpost.salmon.web.tag.ui.FixLengthTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>limit</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>src</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>postfix</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>escapeXml</name>
            <required>false</required>
        </attribute>
    </tag>
    
    <tag>
        <name>pager</name>
        <tag-class>com.srpost.salmon.web.tag.ui.PagerTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>pagerBean</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>script</name>
            <required>false</required>
        </attribute>
        <attribute>
            <name>page</name>
            <required>false</required>
        </attribute>
    </tag>
    
    <tag>
        <name>xss</name>
        <tag-class>com.srpost.salmon.web.tag.ui.XssTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <name>mask-word</name>
        <tag-class>com.srpost.salmon.web.tag.ui.MaskWordTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>word</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>'â—‹' (default)</description>
            <name>mask</name>
            <required>false</required>
        </attribute>
    </tag>
    
    <tag>
        <name>image</name>
        <tag-class>com.srpost.salmon.web.tag.ui.ImageTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>fileList</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>fileBean</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>fileUrl</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>title</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>useXr</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <name>new-icon</name>
        <tag-class>com.srpost.salmon.web.tag.ui.NewIconTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>date</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>day</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <name>loc</name>
        <tag-class>com.srpost.salmon.web.tag.ui.FrontMenuCurrentTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <description>menu code</description>
            <name>menuCd</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>loc list for compare</description>
            <name>locList</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>css class name</description>
            <name>cssClass</name>
        </attribute>
    </tag>
    
    
    <!-- Spring override -->
    <tag>
        <name>msg</name>
        <tag-class>org.springframework.web.servlet.tags.MessageTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <name>message</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>code</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>arguments</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>argumentSeparator</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>text</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>var</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>scope</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>htmlEscape</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>javaScriptEscape</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
</taglib>