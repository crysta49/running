package com.srpost.cm.bo.base.cache;

import org.apache.ibatis.type.Alias;

import com.srpost.salmon.bean.BaseBean;

/**
 * 내부단 캐시 정보 Bean
 *
 * @author  finkle
 * @date    2014-11-11
 * @since   2.0
 */
@Alias("cacheBean")
@SuppressWarnings("serial")
public class CacheBean extends BaseBean {
    
    /** 일련번호 */
    private Integer seq;
    /** IP_주소 */
    private String ipAddr;
    /** CLASS_명 */
    private String classNm;
    /** RELOAD_일시 */
    private String reloadDt;
    
    
    public Integer getSeq() {
        return seq;
    }
    public void setSeq(Integer seq) {
        this.seq = seq;
    }
    public String getIpAddr() {
        return ipAddr;
    }
    public void setIpAddr(String ipAddr) {
        this.ipAddr = ipAddr;
    }
    public String getClassNm() {
        return classNm;
    }
    public void setClassNm(String classNm) {
        this.classNm = classNm;
    }
    public String getReloadDt() {
        return reloadDt;
    }
    public void setReloadDt(String reloadDt) {
        this.reloadDt = reloadDt;
    }
}
